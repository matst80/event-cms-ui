{
	"compileOnSave": true,
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"rootDir": ".",
		"target": "ESNext",
		"outDir": "temp",
		"lib": [
			"ES2020",
			"dom"
		],
		"sourceMap": true,
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"module": "ESNext",
		"moduleResolution": "node",
		"noImplicitReturns": true,
		"strict": true,
		"suppressImplicitAnyIndexErrors": true,
		"noImplicitAny": false
	}
}